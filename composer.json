{
  "name": "rob-lester-jr04/laravel-sendle",
  "description": "A Laravel wrapper for the Sendle shipping API",
  "license": "MIT",
  "keywords": [
	"eloquent",
	"sendle",
	"wrapper",
	"api",
	"model",
	"laravel"
  ],
  "type": "library",
  "authors": [
	{
	  "name": "Robert Lester",
	  "email": "roblesterjr04@gmail.com"
	}
  ],
  "require": {
    "php": "^8.1 || ^8.2",
    "fakerphp/faker": "^1.23",
    "guzzlehttp/guzzle": "^6.0 || ^7.0",
    "laravel/framework": "^9.0 || ^10.0"
  },
  "require-dev": {
	"phpunit/phpunit": ">=6.0 || >=10.0"
  },
  "autoload": {
	"psr-4": {
	  "Sendle\\": "src"
	}
  },
  "autoload-dev": {
	"psr-4": {
	  "Sendle\\Tests\\": "tests"
	}
  },
  "scripts": {
	"test": "vendor/bin/phpunit --bootstrap ./vendor/autoload.php",
	"post-autoload-dump": [
		"Illuminate\\Foundation\\ComposerScripts::postAutoloadDump"
	],
	"docs": "docsify serve ./docs"
  },
  "extra": {
	"laravel": {
	  "providers": [
		"Sendle\\ServiceProvider"
	  ],
	  "aliases": {
		"Sendle": "Sendle\\Facades\\Sendle"
	  }
	}
  },
  "config": {
	"preferred-install": "dist",
	"sort-packages": true,
	"optimize-autoloader": true
  }
}
